<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}</title>
</head>

<body>
    <main>
        <h1>User List</h1>
        <% if (users && users.length > 0) { %>
            <ul>
                <% users.forEach(function(user) { %>
                    <li>
                        Username: <%= user.username %>
                    </li>
                    <li>
                        Full Name: <%= user.fullname %>
                    </li>
                    <br>
                <% }); %>
            </ul>
        <% } else { %>
            <p>No users found.</p>
        <% } %>
    </main>
</body>

</html>
